{
    "name": "digiworld/digichat",
    "description": "Laravel package for DigiChat WhatsApp API integration",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "whatsapp",
        "api",
        "digichat"
    ],
    "homepage": "https://chat.digiworld-dev.com/",
    "support": {
        "email": "info@digiworld-dev.com",
        "issues": "https://chat.digiworld-dev.com/",
        "source": "https://chat.digiworld-dev.com/"
    },
    "authors": [
        {
            "name": "DigiChat",
            "email": "info@digiworld-dev.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Digiworld\\DigiChat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digiworld\\DigiChat\\DigiChatServiceProvider"
            ],
            "aliases": {
                "DigiChat": "Digiworld\\DigiChat\\Facades\\DigiChat"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}